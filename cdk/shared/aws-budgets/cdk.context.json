    },
    "uat": {
      "account_alias": "{INSERT_AWS_ACCOUNT_ALIAS}",
      "account_id": "{INSERT_AWS_ACCOUNT_ID}",
      "region": "eu-west-2"
    },
    "prod": {
      "account_alias": "{INSERT_AWS_ACCOUNT_ALIAS}",
      "account_id": "{INSERT_AWS_ACCOUNT_ID}",
      "region": "eu-west-2"
    }
  },
  "tags": {
    "caution": "Created with IaC - Do not modify on the console!",
    "code": "https://github.com/donovan-said/aws-cdk-examples",
