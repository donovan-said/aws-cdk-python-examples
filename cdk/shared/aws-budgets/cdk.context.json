{
  "stack": {
    "budget_stack": {
      "name": "{APP_NAME}-Budget-Stack",
      "description": "This stack contains all the infrastructure for the Budget solution."
    }
  },
  "account": {
    "dev": {
      "account_alias": "{INSERT_AWS_ACCOUNT_ALIAS}",
